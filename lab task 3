class length{
    public static void main(String[] args){
        String text= "arindom chakraborty";
        System.out.println("the length of the text is" + text.length());
    }
}

class Case {
    public static void main(String[] args){
        String text= "arindom";
        System.out.println("the upper cse is "+ text.toUpperCase());
        System.out.println("the lower case letters are" + text.toLowerCase());
        }

}

class Indexof{
    public static void main(String[] args){
        String text = "arindom chakraborty";
        System.out.println(text.indexOf("chakraborty"));
    }
}
class Compare{
    public static void main(String[] args){
        String text1 = "arindom";
        String text2 = "chakraborty";

        String text3 = "aritra";
        String text4 = "aritra";
        System.out.println(text1.equals(text2));
        System.out.println(text3.equals(text4));

    }
}

class Trim{
    public static void main(String[] args){
        String text = "  arindom  chakraborty";
        System.out.println(text);
        System.out.println(text.trim());



    }
}

class Concatenation{
    public static void main(String[] args){
        String text = "arindom chakraborty";
        String age= "24";
        System.out.println( text + " " + age);
    }
}

class Concat2{
    public static void main(String[] args){
        String text = "arindom";
        String Text = "chakrabory";
        String age = "24";
        String result =text.concat(Text);
        System.out.println(" " + result + " ");
    }
}

class numstring {
    public static void main(String[] args){
        int x = 10;
        int y = 20;
        int z = x + y;
        String x1 = "10";
        String y2 = "20";
        String z3 = x1 + y2;
        System.out.println(z);
        System.out.println(z3);
    }
}

